<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.warehouse.dao.Goods_WarehouseMapper">

    <insert id="addGoods_Warehouse" parameterType="Integer">
        insert into goods_warehouse (lid,wid,gid) values (null,#{wid},#{gid})
    </insert>
    <update id="updateGoods_Warehouse" parameterType="Integer">
        update goods_warehouse set wid=#{wid} where gid=#{gid}
    </update>
</mapper>